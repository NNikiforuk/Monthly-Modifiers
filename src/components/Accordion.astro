---
import "./accordion.scss";

const { faq } = Astro.props;
---

<section class="accordion">
	<ul>
		{
			faq.map((el: { title: string; answer: string }) => (
				<li>
					<input type="checkbox" checked />
					<i />
					<h2>{el.title}</h2>
					<p>{el.answer}</p>
				</li>
			))
		}
	</ul>
</section>
